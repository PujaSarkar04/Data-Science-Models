   "cell_type": "code",
   "execution_count": 124,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Function for one hot encoding as Embarked is an object here.\n",
    "\n",
    "def encode_features(df_train):\n",
    "    features = ['Pclass','Sex','Age','SibSp','Parch','Fare','Embarked']\n",
    "    df_combined = df_train[features].copy()\n",
    "    for feature in features:\n",
    "        le = preprocessing.LabelEncoder()\n",
    "        le = le.fit(df_combined[feature])\n",
    "        df_train[feature] = le.transform(df_train[feature])\n",
    "        #df_test[feature] = le.transform(df_test[feature])\n",
    "    return df_train"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 129,
   "metadata": {},
   "outputs": [],
